mixin sectionTitle(title)
  table.full_width(width="600" border="0" cellpadding="0" cellspacing="0" align="center")
    tr
      td.double_padding_top(align="center")
        p.section_title= title
    tr
      td.double_padding_bottom.single_padding_top(align="center")
        table(width="50" border="0" cellpadding="0" cellspacing="0" align="center")
          tr
            td
              img(src="http://media.sailthru.com/37s/1k0/3/2/56d6d3b0181db.gif" width="100%" height="1")